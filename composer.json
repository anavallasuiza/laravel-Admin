{
    "name": "anavallasuiza/laravel-admin",
    "keywords": ["laravel", "admin"],
    "description": "Simple and extendible Laravel control panel",
    "license": "MIT",
    "authors": [
        {
            "name": "Lito",
            "email": "lito@eordes.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "almasaeed2010/adminlte": "2.*",
        "form-manager/form-manager": "master-dev",
        "gettext/gettext": "3.*",
        "laravel/packer": "master-dev",
        "laravel/meta": "master-dev"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/anavallasuiza/laravel-Admin/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan admin:publish:assets"
        ]
    },
    "minimum-stability": "dev"
}
